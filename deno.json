{
	"tasks": {
		"publish": {
			"command": "cargo publish --allow-dirty",
			"dependencies": ["js"]
		},
		"js": "rollup -p @rollup/plugin-node-resolve public/cool-stylesheet.js | terser --mangle --compress -o dist/cool-stylesheet.js"
	},
	"nodeModulesDir": "auto",
	"imports": {
		"@rollup/plugin-node-resolve": "npm:@rollup/plugin-node-resolve@^16.0.1",
		"handcraft": "npm:handcraft@next",
		"rollup": "npm:rollup@^4.43.0",
		"terser": "npm:terser@^5.43.0"
	}
}
